version: '3'
services:
  node1:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO7-collect-agregate-node1.txt
       args:
          MY_PYTHON_VERSION: "${MY_PYTHON_VERSION}"
    networks:
       - module-agregate
    cap_add:
       - ALL

  node2:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO7-collect-agregate-node2.txt
       args:
          MY_PYTHON_VERSION: "${MY_PYTHON_VERSION}"
    networks:
       - module-agregate
    cap_add:
       - ALL


networks:
  module-agregate:
      driver: bridge

