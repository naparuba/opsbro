version: '3'
services:
  node1:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO11-executor-1.txt
       args:
          MY_PYTHON_VERSION: "${MY_PYTHON_VERSION}"
    networks:
       - executor
    cap_add:
       - ALL


  node2:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO11-executor-2.txt
       args:
          MY_PYTHON_VERSION: "${MY_PYTHON_VERSION}"
    networks:
       - executor
    cap_add:
       - ALL


  node3:
    build:
       context: ../..
       dockerfile: test/docker-files/docker-file-DUO11-executor-3.txt
       args:
          MY_PYTHON_VERSION: "${MY_PYTHON_VERSION}"
    networks:
       - executor
    cap_add:
       - ALL


networks:
  executor:
      driver: bridge

